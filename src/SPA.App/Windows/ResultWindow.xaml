<Window x:Class="SPA.App.ResultWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:SPA.App"
        Title="Results" Height="425" Width="600" ResizeMode="NoResize"  
        Icon="../Resources/ant.ico">
    <StackPanel>
        <TextBox x:Name="LogTextBox" VerticalScrollBarVisibility="Auto" HorizontalScrollBarVisibility="Auto"  VerticalAlignment="Top" Margin="10,10,10,10" IsReadOnly="True" AcceptsReturn="True" Height="335"/>
        <Button x:Name="ReturnButton" Content="Return" VerticalAlignment="Bottom" HorizontalAlignment="Center" Visibility="Collapsed" Click="ReturnButton_Click" Style="{StaticResource AppButtonStyle}"/>
    </StackPanel>
</Window>
